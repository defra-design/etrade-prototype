{% set id = params.id %}
{% set headingLevel = params.headingLevel if params.headingLevel else 2 %}
{% set headingLink = params.headingLink if params.headingLink else "#" %}
{% set partOfProcess = params.partOfProcess if params.partOfProcess else true %}

<!--{% set xxxx = params.xxxx if params.xxxx else yyyy %}-->

<div class="defra-step-by-step-section-sidebar">          
    <div class="app-step-nav-related app-step-nav-related--singular">
        <h2 class="app-step-nav-related__heading">
            <span class="app-step-nav-related__pretitle">Part of</span>
            <a href="index.html">Defra developer portal API step by step</a>
        </h2>
        {# <span class="defra-step-by-step-navigation-show-all-steps">Show all</span> #}
    </div>
    <div id="step-by-step-navigation-default-ip-guide" class="defra-step-nav app-step-nav">
      <ol class="app-step-nav__steps">
        <li aria-current="step" data-show id="introduction" class="app-step-nav__step
        {% if params.currentDefaultStep == 1 %} 
          app-step-nav__step--active
        {% endif %} 
        ">
          <div class="app-step-nav__header " data-position="1">
              <h3 class="app-step-nav__title">
                <span class="app-step-nav__circle app-step-nav__circle--number">
                    <span class="app-step-nav__circle-inner">
                    <span class="app-step-nav__circle-background">
                        <span class="app-step-nav__circle-step-label govuk-visually-hidden">Step</span> 1
                        <span class="app-step-nav__circle-step-colon govuk-visually-hidden" aria-hidden="true">:</span>
                    </span>
                    </span>
                </span>
                <span>
                    Introduction
                </span>
              </h3>
          </div>
          <div class="app-step-nav__panel js-panel" id="step-panel-introduction-1">
              <p class="app-step-nav__paragraph">Get an overview of what the API developer portal is and how you can use it.</p>
              <p class="app-step-nav__paragraph"><a href="introduction.html" class="app-step-nav__link">Introduction</a></p>
          </div>
        </li>
        <li aria-current="step" data-show id="application-naming-guidelines" class="app-step-nav__step
        {% if params.currentDefaultStep == 2 %} 
          app-step-nav__step--active
        {% endif %} 
        ">
          <div class="app-step-nav__header " data-position="2">
              <h3 class="app-step-nav__title">
                <span class="app-step-nav__circle app-step-nav__circle--number">
                    <span class="app-step-nav__circle-inner">
                    <span class="app-step-nav__circle-background">
                        <span class="app-step-nav__circle-step-label govuk-visually-hidden">Step</span> 2
                        <span class="app-step-nav__circle-step-colon govuk-visually-hidden" aria-hidden="true">:</span>
                    </span>
                    </span>
                </span>
                <span>
                    Application naming guidelines
                </span>
              </h3>
          </div>
          <div class="app-step-nav__panel js-panel" id="step-panel-application-naming-guidelines-1">
              <p class="app-step-nav__paragraph">Find out how to name your applications that use the Defra APIs</p>
              <p class="app-step-nav__paragraph"><a href="application-naming-guidelines.html" class="app-step-nav__link">Application naming guidelines</a></p>
          </div>
        </li>
        <li aria-current="step" data-show id="reference-guide" class="app-step-nav__step
          {% if params.currentDefaultStep == 3 %} 
              app-step-nav__step--active
          {% endif %} 
        ">
            <div class="app-step-nav__header " data-position="3">
                <h3 class="app-step-nav__title">
                <span class="app-step-nav__circle app-step-nav__circle--number">
                    <span class="app-step-nav__circle-inner">
                    <span class="app-step-nav__circle-background">
                        <span class="app-step-nav__circle-step-label govuk-visually-hidden">Step</span> 3
                        <span class="app-step-nav__circle-step-colon govuk-visually-hidden" aria-hidden="true">:</span>
                    </span>
                    </span>
                </span>
                <span>
                    Reference guide
                </span>
                </h3>
            </div>
            <div class="app-step-nav__panel js-panel" id="step-panel-reference-guide-1">
                <p class="app-step-nav__paragraph">Documentation about all aspects of the API.</p>
                <p class="app-step-nav__paragraph"><a href="reference-guide.html" class="app-step-nav__link">Reference guide</a></p>
            </div>
        </li>
        <li aria-current="step" data-show id="authorisation" class="app-step-nav__step
        {% if params.currentDefaultStep == 4 %} 
            app-step-nav__step--active
        {% endif %} 
        ">
            <div class="app-step-nav__header " data-position="4">
                <h3 class="app-step-nav__title">
                  <span class="app-step-nav__circle app-step-nav__circle--number">
                      <span class="app-step-nav__circle-inner">
                      <span class="app-step-nav__circle-background">
                          <span class="app-step-nav__circle-step-label govuk-visually-hidden">Step</span> 4
                          <span class="app-step-nav__circle-step-colon govuk-visually-hidden" aria-hidden="true">:</span>
                      </span>
                      </span>
                  </span>
                  <span>
                      Authorisation
                  </span>
                </h3>
            </div>
            <div class="app-step-nav__panel js-panel" id="step-panel-authorisation-1">
                <p class="app-step-nav__paragraph">Guidance on how to allow users into your applications</p>
                <p class="app-step-nav__paragraph"><a href="authorisation.html" class="app-step-nav__link">Authorisation</a></p>
            </div>
        </li>
        <li aria-current="step" data-show id="sample-project" class="app-step-nav__step
          {% if params.currentDefaultStep == 5 %} 
              app-step-nav__step--active
          {% endif %} 
        ">
            <div class="app-step-nav__header " data-position="5">
                <h3 class="app-step-nav__title">
                <span class="app-step-nav__circle app-step-nav__circle--number">
                    <span class="app-step-nav__circle-inner">
                    <span class="app-step-nav__circle-background">
                        <span class="app-step-nav__circle-step-label govuk-visually-hidden">Step</span> 5
                        <span class="app-step-nav__circle-step-colon govuk-visually-hidden" aria-hidden="true">:</span>
                    </span>
                    </span>
                </span>
                <span>
                    Sample project
                </span>
                </h3>
            </div>
            <div class="app-step-nav__panel js-panel" id="step-panel-sample-project-1">
                <p class="app-step-nav__paragraph">View a sample project and read the accompanying guidance documentation.</p>
                <p class="app-step-nav__paragraph"><a href="sample-project.html" class="app-step-nav__link">Sample project</a></p>
            </div>
        </li>
        <li aria-current="step" data-show id="sample-project" class="app-step-nav__step
          {% if params.currentDefaultStep == 6 %} 
              app-step-nav__step--active
          {% endif %} 
        ">
            <div class="app-step-nav__header " data-position="6">
                <h3 class="app-step-nav__title">
                <span class="app-step-nav__circle app-step-nav__circle--number">
                    <span class="app-step-nav__circle-inner">
                    <span class="app-step-nav__circle-background">
                        <span class="app-step-nav__circle-step-label govuk-visually-hidden">Step</span> 6
                        <span class="app-step-nav__circle-step-colon govuk-visually-hidden" aria-hidden="true">:</span>
                    </span>
                    </span>
                </span>
                <span>
                    Terms of use
                </span>
                </h3>
            </div>
            <div class="app-step-nav__panel js-panel" id="step-panel-terms-of-use-1">
                <p class="app-step-nav__paragraph">Find out about the terms of use for consuming the Defra API.</p>
                <p class="app-step-nav__paragraph"><a href="terms-of-use.html" class="app-step-nav__link">Terms of use</a></p>
            </div>
        </li>
      </ol>
    </div>
</div>